<ul>
  <li><%= t("Station Name") %>: <h1><%= @station.name %></h1></li>
  <li><%= t("Dock Count") %>: <%= @station.dock_count %></li>
  <li><%= t("City") %>: <%= @station.city %></li>
  <li><%= t("Installation Date") %>: <%= @station.installation_date %></li>
  <% if !current_user.nil? %>
    <li><%= t("Number of Rides Started at this Station") %>: <%= @station.started_at %></li>
    <li><%= t("Number of Rides Ended at this Station") %>: <%= @station.ended_at %></li>
    <li><%= t("Popular Destination Station") %>: <%= @station.popular_end_station %></li>
    <li><%= t("Popular Starting Station") %>: <%= @station.popular_start_station %></li>
    <li><%= t("Popular Starting Date") %>: <%= @station.popular_date %></li>
    <li><%= t("Popular Zip Code of Users Starting at this Station") %>: <%= @station.popular_zip %></li>
    <li><%= t("Bike ID most frequently starting a trip at this station") %>: <%= @station.popular_bike %></li>
  <%end%>
  <% if !current_user.nil? && current_user.admin? %>
      <%= link_to t('Edit'), edit_admin_station_path(@station) %>
      <%= link_to t("Delete"), admin_station_path(@station), method: "delete" %>
    <%end%>
</ul>
